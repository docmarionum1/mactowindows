{
    "title": "Custom",
    "rules": [
      {
        "description": "Ctrl+D",
        "manipulators": [
          {
              "type": "basic",
              "from": {
                  "key_code": "c",
                  "modifiers": {
                      "mandatory": [
                          "left_command"
                      ],
                      "optional": [
                        "any"
                      ]
                  }
              },
              "to": [
                  {
                      "key_code": "i",
                      "modifiers": [
                        "left_command"
                      ]
                  }
              ]
          },
  
          {
              "type": "basic",
              "from": {
                  "key_code": "d",
                  "modifiers": {
                      "mandatory": [
                          "left_command"
                      ],
                      "optional": [
                        "any"
                      ]
                  }
              },
              "to": [
                  {
                      "key_code": "h",
                      "modifiers": [
                        "left_command"
                      ]
                  }
              ]
          }
        ]
      },
      {
        "description": "Change Option+Tab to Control+Tab",
        "manipulators": [
            {
                "from": {
                    "key_code": "tab",
                    "modifiers": {
                        "mandatory": [
                            "left_alt"
                        ],
                        "optional": [
                            "any"
                        ]
                    }
                },
                "to": [
                    {
                        "key_code": "tab",
                        "modifiers": [
                            "control"
                        ]
                    }
                ],
                "type": "basic"
            }
        ]
    }
    ]
  }